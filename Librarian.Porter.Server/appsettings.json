{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http1": {
        "Url": "http://*:5001",
        "Protocols": "Http1"
      },
      "Http2": {
        "Url": "http://*:5000",
        "Protocols": "Http2"
      }
    }
  },
  "PorterConfig": {
    "PorterName": "Librarian-Porter-CSharp",
    "Region": "Global",
    "IsSteamEnabled": false,
    "SteamApiKey": "",
    "SteamMinRequestIntervalSeconds": 20,
    "IsBangumiEnabled": false,
    "BangumiApiKey": "",
    "IsVndbEnabled": true,
    "VndbApiKey": ""
  },
  "ConsulConfig": {
    "IsEnabled": true,
    "ConsulAddress": "http://localhost:8500",
    "ServiceName": "porter",
    "ServiceAddress": "localhost",
    "ServicePort": "5000",
    "HealthCheckUrl": "http://localhost:5001/health"
  }
}
